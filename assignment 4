     #include<stdio.h>
int main ( )
{
    int i=1;
    while(i<=10){
        printf("%d ",i);
        i++;
    }
    return 0;
}



 //Q3.Write a program to print the first 10 odd natural numbers
 
 
    code:-
    
        #include<stdio.h>
        int main ( )
{
      int i=1;
       while(i<=10)
   {
        printf("%d ",i*2-1);
        i++;
    }
    return 0;
}
  
  
  
  
  // Q4.Write a program to print the first 10 odd natural numbers in reverse order.
  
  
   code:-
   
   
       #include<stdio.h>
        int main ( )
{
    int i=10;
    while(i>=1)
    {
        printf("%d ",i*2-1);
        i--;
    }
    return 0;
}




// Q6. Write a program to print the first 10 even natural numbers 



code:-
    
    #include<stdio.h>
    int main ( )
{
    int i=1;
    while(i<=10)
    {
        printf("%d ",i*2);
        i++;
    }
    return 0;
}



    // Q7.Write a program to print the first 10 even natural numbers in reverse order 
    
    
      code:-
      
        #include<stdio.h>
         int main ( )
{
    int i=10;
    while(i>=1)
    {
        printf("%d ",i*2);
        i--;
    }
    return 0;
}



//Q8. Write a program to print squares of the first 10 natural numbers 


code:-
    
    #include<stdio.h>
int main ( )
{
    int i=1;
    while(i<=10)
    {
        printf("%d ",i*i);
        i++;
    }
    return 0;
}




//Q9.Write a program to print cubes of the first 10 natural numbers 



code:-

    #include<stdio.h>
int main ( )
{
    int i=1;
    while(i<=10)
    {
        printf("%d ",i*i*i);
        i++;
    }
    return 0;
}





//Q9.Write a program to print cubes of the first 10 natural numbers 


#include<stdio.h>
int main ( )
{
    int i=1;
    while(i<=10)
    {
        printf("%d ",i*i*i);
        i++;
    }
    return 0;
}
  
  
  //Q10.Write a program to print a table of 5.


code:-


#include<stdio.h>
int main ( )
{
    int i=1;
    while(i<=10)
    {
        printf("%d ",i*5);
        i++;
    }
    return 0;
}


  
